package questlab.motter.CFG;

public class DecisionNode extends Node {
	private Condition condi;
	
	public DecisionNode(){
		condi=null;
	}
	
	public DecisionNode(String Id,String code){
		super(Id,code);
	}
	public void setcondition(Condition c){
		condi=c;
	}
	public Condition getcondition(){
		return condi;
	}
}
